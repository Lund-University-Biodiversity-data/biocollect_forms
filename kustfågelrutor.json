{
  "modelName": "Kustfagelrutor",
  "record": "true",
  "title": "Kustfagelrutor",
  "dataModel": [
    {
      "dataType": "text",
      "name": "recordedBy",
      "dwcAttribute": "recordedBy",
      "description": "The name of the person who led the survey event.",
      "validate": "required"
    },
    {
      "allowRowDelete": "true",
      "columns": [
        {
          "dataType": "text",
          "name": "helper"
        }
      ],
      "dataType": "list",
      "name": "helpers"
    },
    {
      "defaultValue": "${now}",
      "dataType": "date",
      "name": "surveyDate",
      "dwcAttribute": "eventDate",
      "validate": "required"
    },
    {
      "dataType": "time",
      "name": "surveyStartTime",
      "dwcAttribute": "eventTime",
      "validate": "required"
    },
    {
      "dataType": "time",
      "name": "surveyFinishTime",
      "dwcAttribute": "eventTime",
      "validate": "required"
    },
    {
      "dataType": "text",
      "name": "eventRemarks",
      "dwcAttribute": "eventRemarks",
      "description": "Anteckningar om avvikelser, överhoppningar, mm"
    },
    {
      "dataType": "text",
      "name": "comments",
      "dwcAttribute": "notes",
      "description": "Anteckningar om fynd av döda fåglar."
    },
    {
      "dataType": "text",
      "name": "separateIslandWater",
      "constraints": [
        "ja",
        "nej"
      ],
      "validate": "required"
    },
    {
      "dataType": "text",
      "name": "transectName",
      "validate": "required"
    },
    {
      "defaultAccuracy": 50,
      "hideMyLocation": false,
      "columns": [
        {
          "dwcAttribute": "verbatimLatitude",
          "source": "locationLatitude"
        },
        {
          "dwcAttribute": "verbatimLongitude",
          "source": "locationLongitude"
        },
        {
          "source": "Locality"
        },
        {
          "source": "Accuracy"
        },
        {
          "source": "Notes"
        },
        {
          "source": "Source"
        }
      ],
      "dataType": "geoMap",
      "name": "location",
      "hideSiteSelection": true,
      "zoomToProjectArea": true,
      "validate": "required"
    },
    {
      "allowRowDelete": "true",
      "columns": [
        {
          "dataType": "species",
          "name": "species",
          "dwcAttribute": "scientificName",
          "validate": "required"
        },
        {
          "dataType": "number",
          "name": "island",
          "validate": "min[0]"
        },
        {
          "dataType": "number",
          "name": "water",
          "validate": "min[0]"
        },
        {
          "dataType": "number",
          "name": "individualCount",
          "dwcAttribute": "individualCount",
          "computed": {
            "operation": "sum",
            "dependents": [
              "water",
              "island"
            ]
          },
          "validate": "required,min[0]"
        }
      ],
      "dataType": "list",
      "name": "observations"
    },
    {
      "dataType": "stringList",
      "name": "mammalObservations",
      "constraints": [
        "nej",
        "mink",
        "rödräv",
        "grävling"
      ],
      "validate": "required"
    },
    {
      "dataType": "text",
      "name": "ducklingsCounted",
      "constraints": [
        "ja",
        "nej"
      ],
      "validate": "required"
    },
    {
      "dataType": "number",
      "name": "ducklingCount"
    },
    {
      "dataType": "number",
      "name": "ducklingSize",
      "constraints": [
        1,
        2,
        3,
        4
      ]
    },
    {
      "dataType": "text",
      "name": "statement",
      "constraints": [
        "ja",
        "nej"
      ],
      "validate": "required"
    },
    {
      "dataType": "text",
      "name": "deviation"
    }
  ],
