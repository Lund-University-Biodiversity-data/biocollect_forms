{
  "modelName": "Slinga",
  "title": "Slinga",
  "record": true,
  "dataModel": [
    {
      "dataType": "date",
      "defaultValue": "${now}",
      "name": "surveyDate",
      "dwcAttribute": "eventDate",
      "validate": "required"
    },
    {
      "dataType": "text",
      "name": "recordedBy",
      "dwcAttribute": "recordedBy",
      "validate": "required"
    },
    {
      "allowRowDelete": "true",
      "columns": [
        {
          "dataType": "text",
          "name": "helper"
        }
      ],
      "dataType": "list",
      "name": "helpers"
    },
    {
      "dataType": "time",
      "name": "surveyStartTime",
      "dwcAttribute": "eventTime",
      "validate": "required"
    },
    {
      "dataType": "time",
      "name": "surveyFinishTime",
      "validate": "required"
    },
    {
      "dataType": "number",
      "defaultValue": "",
      "name": "percentageOfSunlight",
      "behaviour": [
        {
          "condition": "percentageOfSunlight < '0'",
          "type": "conditional_validation",
          "value": {
            "validate":"custom[integer],min[0],max[100]",
            "message":"Minsta möjliga värde är 0"
          }
        }
      ]
    },
    {
      "dataType": "number",
      "name": "temperatureInDegreesCelcius",
      "defaultValue": "",
      "behaviour": [
        {
          "condition": "percentageOfSunlight < '0'",
          "type": "conditional_validation",
          "value": {
            "validate":"custom[integer],min[0],max[40]",
            "message":"Minsta möjliga värde är 0"
          }
        }
      ]
    },
    {
      "dataType": "text",
      "name": "windDirectionCategorical",
      "constraints": [
        "Norr",
        "Nordost",
        "Öster",
        "Sydost",
        "Syd",
        "Sydväst",
        "Väster",
        "Nordväst",
        "Vindstilla"
      ]
    },
    {
      "dataType": "text",
      "name": "windSpeedKmPerHourCategorical",
      "constraints": [
        "0 (Rök stiger rakt upp)",
        "1 (Knappt märkbar för känseln)",
        "2 (Små löv rör sig)",
        "3 (Små kvistar rör sig)",
        "4 (Tunnare grenar rör sig)",
        "5 (Mindre träd svajar)",
        "6 (Stora grenar i rörelse)"
      ]
    },
    {
      "defaultAccuracy": 50,
      "hideMyLocation": false,
      "columns": [
        {
          "dwcAttribute": "verbatimLatitude",
          "source": "locationLatitude"
        },
        {
          "dwcAttribute": "verbatimLongitude",
          "source": "locationLongitude"
        },
        {
          "source": "Locality"
        },
        {
          "source": "Accuracy"
        },
        {
          "source": "Notes"
        },
        {
          "source": "Source"
        }
      ],
      "dataType": "geoMap",
      "name": "location",
      "dwcAttribute": "verbatimCoordinates",
      "hideSiteSelection": true,
      "zoomToProjectArea": true,
      "validate": "required"
    },
    {
      "dataType": "text",
      "name": "notes",
      "dwcAttribute": "eventRemarks"
    },
    {
      "allowRowDelete": "true",
      "columns": [
        {
          "dataType": "species",
          "name": "species",
          "dwcAttribute": "scientificName",
          "validate": "required"
        },
        {
          "dataType": "number",
          "name": "S1",
          "validate":"min[0]",
          "defaultValue": ""
        },
        {
          "dataType": "number",
          "name": "S2",
          "validate":"min[0]",
          "defaultValue": ""
        },
        {
          "dataType": "number",
          "name": "S3",
          "validate":"min[0]",
          "defaultValue": ""
        },
        {
          "dataType": "number",
          "name": "S4",
          "validate":"min[0]",
          "defaultValue": ""
        },
        {
          "dataType": "number",
          "name": "S5",
          "validate":"min[0]",
          "defaultValue": ""
        },
        {
          "dataType": "number",
          "name": "S6",
          "validate":"min[0]",
          "defaultValue": ""
        },
        {
          "dataType": "number",
          "name": "S7",
          "validate":"min[0]",
          "defaultValue": ""
        },
        {
          "dataType": "number",
          "name": "S8",
          "validate":"min[0]",
          "defaultValue": ""
        },
        {
          "dataType": "number",
          "name": "S9",
          "validate":"min[0]",
          "defaultValue": ""
        },
        {
          "dataType": "number",
          "name": "S10",
          "validate":"min[0]",
          "defaultValue": ""
        },
        {
          "dataType": "number",
          "name": "S11",
          "validate":"min[0]",
          "defaultValue": ""
        },
        {
          "dataType": "number",
          "name": "S12",
          "validate":"min[0]",
          "defaultValue": ""
        },
        {
          "dataType": "number",
          "name": "S13",
          "validate":"min[0]",
          "defaultValue": ""
        },
        {
          "dataType": "number",
          "name": "S14",
          "validate":"min[0]",
          "defaultValue": ""
        },
        {
          "dataType": "number",
          "name": "S15",
          "validate":"min[0]",
          "defaultValue": ""
        },
        {
          "dataType": "number",
          "computed": {
            "operation": "sum",
            "dependents": [
              "S1",
              "S2",
              "S3",
              "S4",
              "S5",
              "S6",
              "S7",
              "S8",
              "S9",
              "S10",
              "S11",
              "S12",
              "S13",
              "S14",
              "S15"
            ]
          },
          "name": "individualCount",
          "dwcAttribute": "individualCount",
          "validate": "required,min[1]"
        }
      ],
      "dataType": "list",
      "name": "observations"
    }
  ],
