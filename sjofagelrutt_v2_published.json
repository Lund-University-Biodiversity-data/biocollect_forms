{
  "modelName": "Sjofagel",
  "title": "Sjofagel",
  "record": "true",
  "dataModel": [
    {
      "dataType": "text",
      "name": "period",
      "constraints": [
        "September",
        "Januari"
      ],
      "validate": "required"
    },
    {
      "dataType": "text",
      "name": "recordedBy",
      "dwcAttribute": "recordedBy",
      "validate": "required"
    },
    {
      "allowRowDelete": "true",
      "columns": [
        {
          "dataType": "text",
          "name": "helper"
        }
      ],
      "dataType": "list",
      "name": "helpers"
    },
    {
      "defaultValue": "${now}",
      "dataType": "date",
      "name": "surveyDate",
      "dwcAttribute": "eventDate",
      "validate": "required"
    },
    {
      "dataType": "text",
      "name": "observedFrom",
      "validate": "required",
      "constraints": [
        "land",
        "båt",
        "flyg"
      ]
    },
    {
      "dataType": "text",
      "name": "istäcke",
      "constraints": [
        "0-10%",
        "10-50%",
        "50-99%",
        "100%"
      ],
      "validate": "required"
    },
    {
      "dataType": "text",
      "name": "windDirectionCategorical",
      "constraints": [
        "Norr",
        "Nordost",
        "Öster",
        "Sydost",
        "Syd",
        "Sydväst",
        "Väster",
        "Nordväst",
        "Vindstilla"
      ]
    },
    {
      "dataType": "text",
      "name": "windSpeedKmPerHourCategorical",
      "constraints": [
        "0 (Rök stiger rakt upp)",
        "1 (Knappt märkbar för känseln)",
        "2 (Små löv rör sig)",
        "3 (Små kvistar rör sig)",
        "4 (Tunnare grenar rör sig)",
        "5 (Mindre träd svajar)",
        "6 (Stora grenar i rörelse)"
      ]
    },
    {
      "dataType": "text",
      "name": "eventRemarks",
      "dwcAttribute": "eventRemarks",
      "description": "Skriv gärna sådant du tror kommande inventerare kan ha nytta av"
    },
    {
      "dataType": "text",
      "name": "comments",
      "dwcAttribute": "notes"
    },
    {
      "dataType": "text",
      "name": "transectName",
      "validate": "required"
    },
    {
      "defaultAccuracy": 50,
      "hideMyLocation": false,
      "columns": [
        {
          "dwcAttribute": "verbatimLatitude",
          "source": "locationLatitude"
        },
        {
          "dwcAttribute": "verbatimLongitude",
          "source": "locationLongitude"
        },
        {
          "source": "Locality"
        },
        {
          "source": "Accuracy"
        },
        {
          "source": "Notes"
        },
        {
          "source": "Source"
        }
      ],
      "dataType": "geoMap",
      "name": "location",
      "hideSiteSelection": true,
      "zoomToProjectArea": true,
      "validate": "required"
    },
    {
      "dataType": "text",
      "name": "noSpecies",
      "constraints": [
        "ja",
        "nej"
      ],
      "validate": "required"
    },
    {
      "allowRowDelete": "true",
      "columns": [
        {
          "dataType": "species",
          "name": "species",
          "dwcAttribute": "scientificName",
          "validate": "required"
        },
        {
          "dataType": "text",
          "name": "swedishRank"
        },
        {
          "dataType": "number",
          "name": "individualCount",
          "dwcAttribute": "individualCount"
        }
      ],
      "dataType": "list",
      "name": "observations"
    },
    {
      "dataType": "number",
      "name": "totalCount",
      "computed": {
        "expression": "sum(observations, \"individualCount\")"
      }
    },
    {
      "dataType": "number",
      "name": "totalSpeciesCount",
      "computed": {
        "expression": "count(observations, \"species\")"
      }
    }
  ],
  "viewModel": [
    {
